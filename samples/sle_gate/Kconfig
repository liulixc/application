config SAMPLE_SUPPORT_SLE_SERVER_ADC
    bool
    prompt "Support SLE_SERVER_ADC Sample."
    default n
    depends on ENABLE_SLE_GATE_SAMPLE
    help
        This option means support SLE_SERVER_ADC Sample.

